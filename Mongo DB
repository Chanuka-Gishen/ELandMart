 FileWriter fileWriter = new FileWriter("");
        BufferedWriter bufferedWriter = new BufferedWriter(fileWriter);
        for (int i = 0; i < gymMembers.size(); i++) {
            bufferedWriter.write("Membership ID     :" + .get(i).getMembershipNumber());
            bufferedWriter.newLine();
            bufferedWriter.write("Name              :" + .get(i).getName());
            bufferedWriter.newLine();
            bufferedWriter.write("Start Day         :" + .get(i).getStartMemberShipDate());
            bufferedWriter.newLine();

            if(gymMembers.get(i) instanceof StudentMember)
            {
                bufferedWriter.write("Membership Type   : Student Member");
                bufferedWriter.newLine();
                bufferedWriter.write("School Name       :" + ((StudentMember) .get(i)).getSchoolName());
                bufferedWriter.newLine();
                bufferedWriter.write("Parent or Guardian:" + ((StudentMember) .get(i)).getParentOrGuardianName());
                bufferedWriter.newLine();
                bufferedWriter.write("Parent/Guardian No:" + ((StudentMember) .get(i)).getParentOrGuardianContactNumber());
                bufferedWriter.newLine();
            }
            else if(gymMembers.get(i) instanceof Over60Member)
            {
                bufferedWriter.write("Membership Type   : Over 60 Member");
                bufferedWriter.newLine();
                bufferedWriter.write("Age               :" + ((Over60Member) .get(i)).getAge());
                bufferedWriter.newLine();
                bufferedWriter.write("Guardian Name     :" + ((Over60Member) .get(i)).getGuardianName());
                bufferedWriter.newLine();
                bufferedWriter.write("Guardian No       :" + ((Over60Member) gymMembers.get(i)).getGuardianContactNumber());
                bufferedWriter.newLine();
                bufferedWriter.write("Health Condition  :" + ((Over60Member) gymMembers.get(i)).getHealthCondition());
                bufferedWriter.newLine();
            }
            else{
                bufferedWriter.write("Membership Type   : Default Member");
                bufferedWriter.newLine();
            }
            bufferedWriter.write("Address           :" + gymMembers.get(i).getAddress());
            bufferedWriter.newLine();
            bufferedWriter.write("Contact number    :" + gymMembers.get(i).getContactNumber());
            bufferedWriter.newLine();
            bufferedWriter.write("Weight            :" + gymMembers.get(i).getWeight());
            bufferedWriter.newLine();
            bufferedWriter.write("Height            : " + gymMembers.get(i).getHeight());
            bufferedWriter.newLine();
            bufferedWriter.write("-----------------------------------------------------------");
            bufferedWriter.newLine();

        }

        System.out.println("***********_Successfully Saved_**************** ");

        System.out.println(" ");
        bufferedWriter.close();


    } catch (Exception e){
        System.out.println("Error Please check");
    }
}
}
